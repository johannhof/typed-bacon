{
  "name": "typed-bacon",
  "version": "0.1.0",
  "description": "Type checks in your Bacon stream!",
  "main": "dist/typed-bacon.js",
  "scripts": {
    "build": "coffee -c -o dist/ typed-bacon.coffee",
    "test": "NODE_PATH=./ coffee -l README.litcoffee && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johannhof/typed-bacon.git"
  },
  "keywords": [
    "bacon",
    "baconjs",
    "typing",
    "types",
    "stream"
  ],
  "author": "Johann Hofmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johannhof/typed-bacon/issues"
  },
  "homepage": "https://github.com/johannhof/typed-bacon",
  "devDependencies": {
    "baconjs": "^0.7.53",
    "coffee-script": "^1.9.1",
    "jest-cli": "^0.4.0"
  },
  "jest": {
    "testDirectoryName": "tests",
    "scriptPreprocessor": "preprocessor.js",
    "testFileExtensions": [
      "coffee",
      "litcoffee",
      "coffee.md",
      "js"
    ]
  }
}
